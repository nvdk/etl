@prefix textHolder: <http://plugins.linkedpipes.com/ontology/e-textHolder#> .
@prefix configuration: <http://plugins.linkedpipes.com/ontology/configuration/> .
@prefix lp: <http://linkedpipes.com/ontology/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix config: <http://plugins.linkedpipes.com/ontology/configuration/> .

<http://etl.linkedpipes.com/resources/components/e-textHolder/0.0.0> {

    <http://etl.linkedpipes.com/resources/components/e-textHolder/0.0.0> a lp:JarTemplate ;
        <http://www.w3.org/2004/02/skos/core#prefLabel> "Text holder" ;
        lp:keyword "file", "text" ;
        lp:color "#F6D8CE" ;
        lp:componentType <http://etl.linkedpipes.com/ontology/component/type/Extractor> ;
        lp:configurationGraph <http://etl.linkedpipes.com/resources/components/e-textHolder/0.0.0/configuration> ;
        lp:configurationDescription <http://etl.linkedpipes.com/resources/components/e-textHolder/0.0.0/configuration-description> ;
        lp:infoLink <https://etl.linkedpipes.com/components/e-textholder> ;
        lp:supportControl "true"^^<http://www.w3.org/2001/XMLSchema#boolean> ;
        lp:port <http://etl.linkedpipes.com/resources/components/e-textHolder/0.0.0/port/FilesOutput> ;
        lp:jar <http://etl.linkedpipes.com/resources/jars/e-textHolder/0.0.0> ;
        lp:requirement <http://linkedpipes.com/resources/requirement/workingDirectory> ;
        lp:dialog
              <http://etl.linkedpipes.com/resources/components/e-textHolder/0.0.0/dialog/template> ,
              <http://etl.linkedpipes.com/resources/components/e-textHolder/0.0.0/dialog/instance> ,
              <http://etl.linkedpipes.com/resources/components/e-textHolder/0.0.0/dialog/config> ;
    .

    <http://etl.linkedpipes.com/resources/components/e-textHolder/0.0.0/port/FilesOutput> a lp:Output, <http://linkedpipes.com/ontology/dataUnit/system/1.0/files/DirectoryMirror> ;
        lp:binding "FilesOutput" ;
        skos:prefLabel "Output" ;
        lp:requirement <http://linkedpipes.com/resources/requirement/workingDirectory> ;
    .

    <http://etl.linkedpipes.com/resources/components/e-textHolder/0.0.0/dialog/template> a lp:Dialog ;
        lp:name "template" .

    <http://etl.linkedpipes.com/resources/components/e-textHolder/0.0.0/dialog/instance> a lp:Dialog ;
        lp:name "instance" .

    <http://etl.linkedpipes.com/resources/components/e-textHolder/0.0.0/dialog/config> a lp:Dialog ;
        lp:name "config" .

}

<http://etl.linkedpipes.com/resources/components/e-textHolder/0.0.0/configuration> {
    <http://etl.linkedpipes.com/resources/components/e-textHolder/0.0.0/configuration> a textHolder:Configuration ;
        textHolder:content "" ;
        textHolder:contentControl <http://plugins.linkedpipes.com/resource/configuration/None> ;
        textHolder:fileName "" ;
        textHolder:fileNameControl <http://plugins.linkedpipes.com/resource/configuration/None> ;
    .
}

<http://etl.linkedpipes.com/resources/components/e-textHolder/0.0.0/configuration-description> {

<http://etl.linkedpipes.com/resources/components/e-textHolder/0.0.0/configuration-description>
    a <http://plugins.linkedpipes.com/ontology/ConfigurationDescription> ;
    config:type textHolder:Configuration ;
    config:member <http://etl.linkedpipes.com/resources/components/e-textHolder/0.0.0/configuration-description/1>,
        <http://etl.linkedpipes.com/resources/components/e-textHolder/0.0.0/configuration-description/2> .

<http://etl.linkedpipes.com/resources/components/e-textHolder/0.0.0/configuration-description/1>
    a config:ConfigurationMember ;
    config:property textHolder:fileName ;
    config:control textHolder:fileNameControl .

<http://etl.linkedpipes.com/resources/components/e-textHolder/0.0.0/configuration-description/2>
    a config:ConfigurationMember ;
    config:property textHolder:content ;
    config:control textHolder:contentControl .

}
