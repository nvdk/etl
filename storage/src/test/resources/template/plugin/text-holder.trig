}

<http://configuration> {

    _:
        a thc:Configuration ;
        thc:content "" ;
        thc:contentControl <http://plugins.linkedpipes.com/resource/configuration/None> ;
        thc:fileName "" ;
        thc:fileNameControl  <http://plugins.linkedpipes.com/resource/configuration/None> ;
    .

}

<http://configuration-description> {

    <http://example>
        a <http://plugins.linkedpipes.com/ontology/ConfigurationDescription> ;
        lpc:type thc:Configuration ;
        lpc:member
            [
                    a lpc:ConfigurationMember ;
                    lpc:property thc:fileName ;
                    lpc:control thc:fileNameControl ;
            ] ,
            <http://example/10> ;
    .

    <http://example/10>
        a lpc:ConfigurationMember ;
        lpc:property thc:content ;
        lpc:control thc:contentControl ;
    .

}

